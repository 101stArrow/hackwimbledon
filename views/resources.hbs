<div class="container">

    <div class="row">

        <div class="col-md-12">

            {{#each resources }}
                <h3>{{ category-title }}</h3>
                <h4>{{ category-description}}</h4>
                <ul class="list-group">
                    
                        {{#each category-links}}
                            <li class="list-group-item"><span class="label label-default">{{this.link-title}}</span> {{this.link-description}} <a href="{{ this.link-url }}">{{ this.link-url }}</a></li>
                        {{/each}}
                </ul>
            {{/each}}

        </div>

    </div>

</div>
